KAAA;AAcA,GAhBA;AAiBA,EAAA,OAAA,EAAA;AACA;AACA,IAAA,QAFA,sBAEA;AAAA;;AACA,WAAA,eAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,KAAA,KAAA,CAAA,QAAA,CAAA,6BAAA,CADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA;AAAA,kBAAA,KAAA,IAAA;AAAA,oBAAA,KAAA,MAAA;AAAA,mBAAA,KAAA,KAAA;AAAA,qBAAA,KAAA,OAAA;AAAA,qBAAA,KAAA,OAAA;AAAA,uBAAA,KAAA,SAAA;AAAA,qBAAA,KAAA;AAAA;AAHA,OAAA,EAIA,IAJA,CAIA,UAAA,IAAA,EAAA;AACA,YAAA,IAAA,IAAA,IAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,MAAA;AACA,SAFA,MAEA;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,MAAA;AACA;AACA,OAVA;AAWA;AAfA;AAjBA,CAAA","sourcesContent":["<template>\n  <!-- 输入任意文本 -->\n  <div>\n    <van-row gutter=\"20\" type=\"flex\" align=\"center\">\n      <van-col span=\"8\">\n        <van-dropdown-menu>\n          <van-dropdown-item v-model=\"village\" :options=\"option1\" size=\"small\"/>\n        </van-dropdown-menu>\n      </van-col>\n      <van-col span=\"10\">\n        <van-cell-group>\n          <van-field v-model=\"endArea\" placeholder=\"输入内容\" size=\"small\"/>\n        </van-cell-group>\n      </van-col>\n      <van-col span=\"6\">\n        <van-button type=\"info\" size=\"small\">搜索</van-button>\n      </van-col>\n    </van-row>\n\n    <van-card price=\"水电工\" currency=\"\" desc=\"邯郸峰峰\" title=\"xxx设计公司\" thumb=\"https://img.yzcdn.cn/vant/ipad.jpeg\" centered=\"true\">\n      <template #tags>\n        <van-tag plain type=\"danger\">标签</van-tag>\n        <van-tag plain type=\"danger\">标签</van-tag>\n      </template>\n      <template #footer>\n        <van-button size=\"mini\">联系他</van-button>\n        <van-button size=\"mini\">查看施工案例</van-button>\n      </template>\n    </van-card>\n\n    <van-card price=\"水电工\" currency=\"\" desc=\"邯郸峰峰\" title=\"xxx设计公司\" thumb=\"https://img.yzcdn.cn/vant/ipad.jpeg\" centered=\"true\">\n      <template #tags>\n        <van-tag plain type=\"danger\">标签</van-tag>\n        <van-tag plain type=\"danger\">标签</van-tag>\n      </template>\n      <template #footer>\n        <van-button size=\"mini\">联系他</van-button>\n        <van-button size=\"mini\">查看施工案例</van-button>\n      </template>\n    </van-card>\n    <van-card price=\"水电工\" currency=\"\" desc=\"邯郸峰峰\" title=\"xxx设计公司\" thumb=\"https://img.yzcdn.cn/vant/ipad.jpeg\" centered=\"true\">\n      <template #tags>\n        <van-tag plain type=\"danger\">标签</van-tag>\n        <van-tag plain type=\"danger\">标签</van-tag>\n      </template>\n      <template #footer>\n        <van-button size=\"mini\">联系他</van-button>\n        <van-button size=\"mini\">查看施工案例</van-button>\n      </template>\n    </van-card>\n    \n  </div>\n</template>\n<script>\nimport Vue from 'vue';\nimport { Field } from 'vant';\nimport { DropdownMenu, DropdownItem } from 'vant';\nimport { Button } from 'vant';\nimport { Col, Row } from 'vant';\nimport { Notify } from 'vant';\nimport { Card } from 'vant';\nVue.use(Notify);\nVue.use(Col);\nVue.use(Row);\nVue.use(Button);\nVue.use(DropdownMenu);\nVue.use(DropdownItem);\nVue.use(Field);\nVue.use(Card);\n\nexport default {\n  data() {\n    return {\n      name: '',\n      idCard: '',\n      phone: '',\n      village: '0',\n      bodyHot: '',\n      startArea: '',\n      endArea: '',\n      option1: [\n        { text: '全部', value: '0' },\n        { text: '施工队', value: '1' },\n        { text: '装修公司', value: '2' }\n      ]\n    };\n  },\n  methods: {\n    // 获取数据列表\n    saveDate () {\n      this.dataListLoading = true\n      this.$http({\n        url: this.$http.adornUrl('/ouvillagerInformation/save'),\n        method: 'post',\n        data: {'name':this.name,'idCard':this.idCard,'phone':this.phone,'village':this.village,'bodyHot':this.bodyHot,'startArea':this.startArea,'endArea':this.endArea}\n      }).then((data) => {\n        if (data && data.code === 0) {\n            this.$notify('记录成功');\n        } else {\n            this.$notify('记录失败');\n        }\n      })\n    },\n  }\n};\n</script>\n"],"sourceRoot":"src/view/renovation/main"}]}